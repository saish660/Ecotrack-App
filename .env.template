# OneSignal Configuration for Median App
# Add these environment variables to your .env file

# OneSignal App ID (get this from your OneSignal dashboard)
ONESIGNAL_APP_ID=your_onesignal_app_id_here

# OneSignal REST API Key (get this from your OneSignal dashboard -> Settings -> Keys & IDs)
ONESIGNAL_REST_API_KEY=your_onesignal_rest_api_key_here

# Existing Firebase configuration (keep these for web users)
FIREBASE_API_KEY=your_firebase_api_key
FIREBASE_AUTH_DOMAIN=your_firebase_auth_domain
FIREBASE_PROJECT_ID=your_firebase_project_id
FIREBASE_STORAGE_BUCKET=your_firebase_storage_bucket
FIREBASE_MESSAGING_SENDER_ID=your_firebase_messaging_sender_id
FIREBASE_APP_ID=your_firebase_app_id
FIREBASE_VAPID_KEY=your_firebase_vapid_key

# Other settings
GEMINI_API_KEY=your_gemini_api_key
CRON_SECRET=your_cron_secret_for_scheduled_notifications